{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "zhigADFtest"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureHDInsightLinkedService_clusterPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterPassword' of 'AzureHDInsightLinkedService'"
		},
		"AzureHDInsightLinkedService_clusterSshPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterSshPassword' of 'AzureHDInsightLinkedService'"
		},
		"AzureHDInsightLinkedService_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureHDInsightLinkedService'"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_hostSubscriptionId": {
			"type": "string",
			"defaultValue": "1e42591f-1f0c-4c5a-b7f2-a268f6105ec5"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_clusterResourceGroup": {
			"type": "string",
			"defaultValue": "zhig"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "2b74f935-c9de-47f4-b171-f1af62fb44b0 "
		},
		"AzureHDInsightLinkedService_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "72f988bf-86f1-41af-91ab-2d7cd011db47"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_clusterUserName": {
			"type": "string",
			"defaultValue": "zhigtest"
		},
		"scheduletrigger_properties_pipeline2_parameters_filename": {
			"type": "string",
			"defaultValue": "emp.txt"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"AzureHDInsightLinkedService_clusterPassword": {
						"value": "[parameters('AzureHDInsightLinkedService_clusterPassword')]"
					},
					"AzureHDInsightLinkedService_clusterSshPassword": {
						"value": "[parameters('AzureHDInsightLinkedService_clusterSshPassword')]"
					},
					"AzureHDInsightLinkedService_servicePrincipalKey": {
						"value": "[parameters('AzureHDInsightLinkedService_servicePrincipalKey')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_hostSubscriptionId": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_hostSubscriptionId')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_clusterResourceGroup": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_clusterResourceGroup')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_tenant": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_tenant')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_clusterUserName": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_clusterUserName')]"
					},
					"scheduletrigger_properties_pipeline2_parameters_filename": {
						"value": "[parameters('scheduletrigger_properties_pipeline2_parameters_filename')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}